<odoo>
    <record id="view_product_combo_line_form" model="ir.ui.view">
        <field name="name">product.combo.line.form</field>
        <field name="model">product.combo.line</field>
        <field name="inherit_id" ref="pos_custom.view_product_combo_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='combo_lines_page']" position="after">
                <page name="page1" string="page1">
                </page>
            </xpath>
        </field>
    </record>
</odoo>
